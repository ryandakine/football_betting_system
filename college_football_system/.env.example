# College Football System - AWS Configuration
# =============================================

# Bankroll
BANKROLL=50000

# API Keys
ODDS_API_KEY=your_odds_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_key_here

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your_aws_account_id

# Lambda Settings
LAMBDA_MEMORY_SIZE=2048
LAMBDA_TIMEOUT=300

# Feature Flags
ENABLE_CLOUDGPU=false
ENABLE_SOCIAL_ANALYSIS=true
ENABLE_WEATHER_ANALYSIS=true

# Agent Settings
AGENT_METRICS_ENABLED=true
AGENT_CFB_SIMS=2000

# Logging
LOG_LEVEL=INFO

# Notification Settings (optional)
SLACK_WEBHOOK_URL=
EMAIL_ALERTS=
